function pow(a, n) {
    var result = a;
    for (var i = 1; i < n; i++) {
        result *= a;
    }
    return result;
}
var x = prompt("a?", '');
var n = prompt("n?", '');
if (n <= 1) {
    alert('Степень ' + n +
        'не поддерживается, введите целую степень, большую 1'
        );
} else {
    alert( pow(a, n) );
}
